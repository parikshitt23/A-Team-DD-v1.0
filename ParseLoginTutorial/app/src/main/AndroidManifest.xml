            android:label="@string/GradeUP" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginSignupActivity" >
        </activity>
        <activity android:name=".Welcome" >
        </activity>
    </application>

</manifest>